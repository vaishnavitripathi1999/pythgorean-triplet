int max=a;
if(b>=max){
    max=b;
}
if(e>=max){
    max=e;
}
if(max==a){
    boolean flag=((b * b+e * e)==(a * a));
    System.out.println(flag);
}else if(max==b){
    boolean flag=((a*a+c*c)==(b*b));
   System.out.println(flag);
